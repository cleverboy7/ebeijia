<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ page import="com.ebeijia.util.Constant"%>
<ipy-template ipy-id="widget">
	<widget>
		<widget-title></widget-title>
		<widget-content></widget-content>
	</widget>
</ipy-template>

<ipy-template ipy-id="widget-button">
	<div class="sizeBtn">
		<span class="fa"></span>
	</div>
</ipy-template>

<ipy-template ipy-id="simple-form">
	<form action="#">
		<div class="form-input-wrapper"></div>
		<div class="button-wrapper"></div>
	</form>
</ipy-template>

<ipy-template ipy-id="simple-form-input">
	<div class="input-wrapper">
		<label></label>
		<div class="input-item">
			<input>
			<div class="required-mark"></div>
		</div>
	</div>
</ipy-template>

<ipy-template ipy-id="simple-form-file">
	<div class="input-wrapper">
		<label></label>
		<div class="input-item file-item">
			<div class="file-name">
				<span class="name"></span>
				<span class="extension"></span>
			</div>
			<button class="btn btn-primary" type="button">
				<span class="fa fa-paperclip"></span> 附件
			</button>
			<input type="file">
			<div class="required-mark"></div>
		</div>
	</div>
</ipy-template>

<ipy-template ipy-id="simple-form-select">
	<div class="input-wrapper">
		<label></label>
		<div class="input-item select-item">
			<select></select>
			<div class="required-mark"></div>
		</div>
	</div>
</ipy-template>

<ipy-template ipy-id="simple-form-textarea">
	<div class="input-wrapper">
		<label></label>
		<div class="input-item textarea-item">
			<textarea></textarea>
			<div class="required-mark"></div>
		</div>
	</div>
</ipy-template>

<ipy-template ipy-id="simple-form-dual-select">
	<div class="input-wrapper">
		<label></label>
		<dual-select class="input-item clear-fix">
			<ul class="unselected-list"></ul>
			<ul class="selected-list"></ul>
			<div class="button-group">
				<button class="btn btn-primary" type="button">&gt;</button>
				<button class="btn btn-primary" type="button">&gt;&gt;</button>
				<button class="btn btn-primary" type="button">&lt;</button>
				<button class="btn btn-primary" type="button">&lt;&lt;</button>
			</div>
			<div class="required-mark"></div>
			<button class="btn btn-primary prev-page" type="button">&lt;</button>
			<button class="btn btn-primary next-page" type="button">&gt;</button>
		</dual-select>
	</div>
</ipy-template>

<ipy-template ipy-id="table-button-modify">
	<button class="btn btn-primary modify" type="button">
		<span class="fa fa-pencil-square"></span> 修改
	</button>
</ipy-template>

<ipy-template ipy-id="table-button-money">
	<button class="btn btn-primary btn-money" type="button">
		<span class="fa fa-money"></span> 发送
	</button>
</ipy-template>

<ipy-template ipy-id="table-button-delete">
	<button class="btn delete" type="button">
		<span class="fa fa-minus-circle"></span> 删除
	</button>
</ipy-template>

<ipy-template ipy-id="table-button-download">
	<button class="btn btn-primary download" type="button">
		<span class="fa fa-cloud-download"></span> 下载
	</button>
</ipy-template>

<ipy-template ipy-id="table-button-play">
	<button class="btn btn-primary play" type="button">
		<span class="fa fa-play"></span> 开启
	</button>
</ipy-template>

<ipy-template ipy-id="table-button-refresh">
	<button class="btn btn-primary refresh" type="button">
		<span class="fa fa-refresh"></span> 重置
	</button>
</ipy-template>

<ipy-template ipy-id="table-button-stop">
	<button class="btn stop" type="button">
		<span class="fa fa-stop"></span> 关闭
	</button>
</ipy-template>