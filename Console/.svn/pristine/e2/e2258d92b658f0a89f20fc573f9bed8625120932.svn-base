*,
:before,
:after {
	box-sizing: inherit;
	margin: 0;
	padding: 0;
	-webkit-transition: background-color 0.6s, border-color 0.6s, box-shadow 0.6s;
	        transition: background-color 0.6s, border-color 0.6s, box-shadow 0.6s;
}

html {
	box-sizing: border-box;
}

body {
	overflow: hidden;
	font-family: "RobotoRegular", "Helvetica Neue", Helvetica, "Microsoft YaHei", sans-serif;
}

a {
	text-decoration: none;
}

ul {
	list-style-type: none;
}

img,
button {
	border: none;
}

button {
	cursor: pointer;
}

.clear-fix:after {
	display: block;
	clear: both;
	content: "";
}

ipy-template {
	display: none;
}

body > header,
body > header > .logo,
body > header > .topbar,
body > nav,
body > main {
	position: fixed;
}

body > header {
	top: 0;
	left: 0;
	right: 0;
	height: 110px;
}

body > header > .logo {
	top: 0;
	left: 0;
	width: 260px;
	height: 110px;
}

body > header > .topbar {
	top: 0;
	left: 260px;
	right: 0;
	overflow: hidden;
	height: 110px;
}

body > nav {
	top: 110px;
	bottom: 0;
	left: -15px;
	overflow-y: auto;
	width: 260px;
}
body.nav-expand > nav {
	left: 0;
}

body > main {
	top: 110px;
	bottom: 0;
	left: 30px;
	right: 0;
	overflow-y: hidden;
}
body.nav-expand > main {
	left: 260px;
}

body > nav,
body > main {
	-webkit-transition-property: left;
	        transition-property: left;
}
body > nav,
body > main,
.breadcrumb-wrapper .nav-switcher,
.breadcrumb-wrapper .nav-switcher > .center-bar,
.breadcrumb-wrapper .nav-switcher:before,
.breadcrumb-wrapper .nav-switcher:after {
	-webkit-transition-duration: 0.6s;
	        transition-duration: 0.6s;
}