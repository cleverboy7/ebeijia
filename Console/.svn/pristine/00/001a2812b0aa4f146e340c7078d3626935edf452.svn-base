.animation-flip {
	-webkit-animation: flip 0.4s;
	        animation: flip 0.4s;
}

.animation-tada,
.animation-tada-hover:hover {
	-webkit-animation: tada 1s ease;
	        animation: tada 1s ease;
}

.animation-nav-bounce {
	-webkit-animation: nav-bounce 0.5s cubic-bezier(0.2,0,0.5,1.2);
	        animation: nav-bounce 0.5s cubic-bezier(0.2,0,0.5,1.2);
}

@-webkit-keyframes flip {
	0% {
		-webkit-transform: rotateY(0);
	}
	100% {
		-webkit-transform: rotateY(-360deg);
	}
}

@keyframes flip {
	0% {
		transform: rotateY(0);
	}
	100% {
		transform: rotateY(-360deg);
	}
}

@-webkit-keyframes tada {
	0%,
	100% {
		-webkit-transform: scale(1);
	}
	10%,
	20% {
		-webkit-transform: scale(0.9) rotate(-3deg);
	}
	30%,
	50%,
	70%,
	90% {
		-webkit-transform: scale(1.1) rotate(3deg);
	}
	40%,
	60%,
	80% {
		-webkit-transform: scale(1.1) rotate(-3deg);
	}
}

@keyframes tada {
	0%,
	100% {
		transform: scale(1);
	}
	10%,
	20% {
		transform: scale(0.9) rotate(-3deg);
	}
	30%,
	50%,
	70%,
	90% {
		transform: scale(1.1) rotate(3deg);
	}
	40%,
	60%,
	80% {
		transform: scale(1.1) rotate(-3deg);
	}
}

@-webkit-keyframes nav-bounce {
	0% {
		-webkit-transform: translateY(800px);
	}
	100% {
		-webkit-transform: translateY(0);
	}
}

@keyframes nav-bounce {
	0% {
		transform: translateY(800px);
	}
	100% {
		transform: translateY(0);
	}
}